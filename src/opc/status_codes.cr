  def self.get_status_description(code) : Tuple(String, String)
    code = code.to_u32
    if description = STATUS_DESCRIPTION[code]?
      return description
    else
      if code & 1 << 31
        return {"Bad", "Unknown StatusCode value: #{code}"}
      elsif code & 1 << 30
        return {"UncertainIn", "Unknown StatusCode value: #{code}"}
      else
        return {"Good", "Unknown StatusCode value: #{code}"}
      end
    end
  end
